<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/FrameLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/choice_background" >

    <Button
        android:id="@+id/choice_ch1"
        android:layout_width="112dp"
        android:layout_height="97dp"
        android:layout_gravity="center"
        android:layout_marginBottom="50dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/choice_ch1" />

    <Button
        android:id="@+id/choice_ch2"
        android:layout_width="112dp"
        android:layout_height="97dp"
        android:layout_gravity="center"
        android:layout_marginRight="70dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/choice_ch2" />

    <Button
        android:id="@+id/choice_ch3"
        android:layout_width="112dp"
        android:layout_height="97dp"
        android:layout_gravity="center"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/choice_ch3" />

    <ImageView
        android:id="@+id/choice_fly"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="35dp" />

    <ImageView
        android:id="@+id/choice_random"
        android:layout_width="92dp"
        android:layout_height="32dp"
        android:layout_marginLeft="450dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/choice_random" />

    <ImageView
        android:id="@+id/choice_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="490dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/choice_back" />

    <ImageView
        android:id="@+id/start_shadow"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:src="@drawable/backgrond_shadow" />

</FrameLayout>
